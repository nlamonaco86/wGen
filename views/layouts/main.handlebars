<body class="bg-light">
    <div class="jumbotron-fluid bg-warning ">
        <div class="container">
            <h1 class="display-2 font-weight-bold">wGen</h1>
            <p class="lead">placeholder text.</p>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div class="col-lg-4">
                <form>
                    <div class="form-group">
                        <label>Name</label>
                        <input type="text" class="form-control" id="name" placeholder="name">
                    </div>
                    <div class="form-group">
                        <label>Choose a Split:</label>
                        <select class="form-control" id="split">
                            <option>Arnold</option>
                            <option>Push / Pull / Legs</option>
                            <option>Bro-Split</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label>Example multiple select</label>
                        <select multiple class="form-control" id="equipment">
                            <option>Barbell</option>
                            <option>Dumbbell</option>
                            <option>Machine</option>
                            <option>Bodyweight</option>
                            <option>Kettlebell</option>
                        </select>
                    </div>
                    <div class="form-group">
                        <label>Volume Level:</label>
                        <select class="form-control" id="volume">
                            <option>Low</option>
                            <option>Medium</option>
                            <option>High</option>
                        </select>
                    </div>
                    <button type="submit" class="btn-large btn-warning">Submit</button>
                </form>
            </div>
            <div class="col-lg-8">
                {{{body}}}
            </div>
        </div>
    </div>




